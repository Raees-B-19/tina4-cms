<head>
    <meta charset='UTF-8'/>
    <meta http-equiv='X-UA-Compatible' content='IE=edge'/>
    <meta name='viewport' content='width=device-width, initial-scale=1.0'/>
    <title>Tina4 Site Builder</title>

    <!-- Libraries (CSS) -->
    <link href='/tina4site/lib/css/bootstrap.min.css' rel='stylesheet'/>
    <link href='/tina4site/lib/css/messagebox.css' rel='stylesheet'/>
    <link href="https://unpkg.com/tabulator-tables@5.2.3/dist/css/tabulator.min.css" rel="stylesheet">
    <!-- Libraries (JS) -->
    <script src='/tina4site/lib/js/popper.min.js'></script>
    <script src='/tina4site/lib/js/bootstrap.min.js'></script>

    <!--Check if jquery is in scope-->
    <script>
      if (typeof $ != 'function') {
          document.write(`<script src='/tina4site/lib/js/jquery.min.js' type='text/javascript'><\/script>`)
      }
    </script>

    <!--Message box-->
    <script src='/tina4site/lib/js/messagebox.js'></script>

    <!--Tabulator-->
    <script src='/tina4site/lib/js/tabulator.min.js'></script>

    <!--Bring GrapeJS to bring lib into scope-->
    <script src='/tina4site/grapejs/grapes.min.js'></script>

    <!--Bring in main theme-->
    <link rel='stylesheet' href='/tina4site/grapejs/grapes.min.css'/>

    <!--Plugin CSS-->
    <link rel="stylesheet" href="/tina4site/grapejs/plugins/gradient/gradient.css"/>
    <link rel="stylesheet" href="/tina4site/grapejs/plugins/code-editor/code-editor.css"/>

    <!--Plugin JS-->
    <script src="/tina4site/grapejs/plugins/form/grapejs-form-v2.01.min.js"></script>
    <script src="/tina4site/grapejs/plugins/export/export-v1.07.min.js"></script>
    <script src="/tina4site/grapejs/plugins/ckeditor/ckeditor-v0.0.10.min.js"></script>
    <script src="/tina4site/grapejs/plugins/image-editor/tui-image-editor-v0.1.3.min.js"></script>
    <script src="/tina4site/grapejs/plugins/blocks-basic/blocks-basic-v0.1.11.js"></script>
    <script src="/tina4site/grapejs/plugins/navbar/navbar-v0.1.5.js"></script>
    <script src="/tina4site/grapejs/plugins/countdown/countdown-v0.1.3.js"></script>
    <script src="/tina4site/grapejs/plugins/gradient/gradient-v2.0.14.js"></script>
    <script src="/tina4site/grapejs/plugins/style-manager/style-fliter-v1.0.1.js"></script>
    <script src="/tina4site/grapejs/plugins/style-manager/style-bg-v1.0.5.js"></script>
    <script src="/tina4site/grapejs/plugins/flex-box/flex-box-v0.1.1.js"></script>
    <script src="/tina4site/grapejs/plugins/lory/slider-v0.1.5.js"></script>
    <script src="/tina4site/grapejs/plugins/tabs/tabs-v1.0.6.js"></script>
    <script src="/tina4site/grapejs/plugins/tool-tip/tool-tip-v0.1.5.js"></script>
    <script src="/tina4site/grapejs/plugins/custom-code/custom-code-v0.1.3.js"></script>
    <script src="/tina4site/grapejs/plugins/code-editor/code-editor-v1.0.20.js"></script>
    <!--Tina4 plugins -->
    <script src="/tina4site/grapejs/plugins/tina4-assets/tina4asset.js"></script>
    <script src="/tina4site/grapejs/plugins/tina4-components/tina4components.js"></script>
    <script src="/tina4site/grapejs/plugins/tina4-storage/tina4storage.js"></script>
    <script src="/tina4site/grapejs/plugins/tina4-page/tina4page.js"></script>
    <script src="/tina4site/grapejs/plugins/tina4-revision/tina4revision.js"></script>

    <!--Configuration -->
    <script src="/tina4site/config/style.opts.js"></script>
    <script src="/tina4site/config/plugin.opts.js"></script>
    <script src="/tina4site/config/buttons.opts.js"></script>
    <script src="/tina4site/config/device.opts.js"></script>
    <!--Prototype override for string "strip" method-->
    <script src="/tina4site/tina4StringPrototype.js"></script>

    <!-- We want the standard grape theme, then before markup definition, override css with our own -->
    <link rel='stylesheet' href='/tina4site/tina4site.scss'/>

</head>
